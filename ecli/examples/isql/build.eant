<?xml version="1.0"?>

<project name="clisql" default="help">

	<description>
		system: "Command Line Interactive SQL for ODBC datasources."

		library: "ECLI : Eiffel Call Level Interface (ODBC) Library. Project SAFE."
		copyright: "Copyright (c) 2001-2004, Paul G. Crismer and others"
		license: "Eiffel Forum License v2 (see forum.txt)"
		date: "$Date: 2006/04/13 09:33:07 $"

	</description>

	<inherit>
		<parent location="${ECLI}/build.eant">
			<redefine target="init_system"/>
			<redefine target="help"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="clisql"/>
		<set name="system_dir" value="${ECLI}/examples/isql"/>
	</target>

	<target name="help">
		<geant target="help2"/>
	</target>
</project>
